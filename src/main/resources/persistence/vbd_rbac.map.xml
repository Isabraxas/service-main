<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/9/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/9/modelMap http://cayenne.apache.org/schema/9/modelMap.xsd"
	 project-version="9">
	<property name="defaultPackage" value="cc.viridian.vdb.rbac.persistent"/>
	<db-entity name="APP">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="40"/>
	</db-entity>
	<db-entity name="AUTH_ADAPTER">
		<db-attribute name="CONFIG" type="LONGVARCHAR"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="200"/>
		<db-attribute name="PK" type="VARCHAR" length="200"/>
	</db-entity>
	<db-entity name="PERMISSION">
		<db-attribute name="APP" type="VARCHAR" length="15"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" length="100"/>
		<db-attribute name="TIME_CREATE" type="TIMESTAMP"/>
		<db-attribute name="TIME_MODIFY" type="TIMESTAMP"/>
		<db-attribute name="USR_CREATE" type="VARCHAR" length="40"/>
		<db-attribute name="USR_MODIFY" type="VARCHAR" length="40"/>
	</db-entity>
	<db-entity name="ROLE">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" isMandatory="true" length="100"/>
		<db-attribute name="TIME_CREATE" type="TIMESTAMP"/>
		<db-attribute name="TIME_MODIFY" type="TIMESTAMP"/>
		<db-attribute name="USR_CREATE" type="VARCHAR" length="40"/>
		<db-attribute name="USR_MODIFY" type="VARCHAR" length="40"/>
	</db-entity>
	<db-entity name="ROLE_PERMISSION">
		<db-attribute name="FK_PERMISSION" type="INTEGER"/>
		<db-attribute name="FK_ROLE" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="TIME_CREATE" type="TIMESTAMP"/>
		<db-attribute name="TIME_MODIFY" type="TIMESTAMP"/>
		<db-attribute name="USR_CREATE" type="VARCHAR" length="40"/>
		<db-attribute name="USR_MODIFY" type="VARCHAR" length="40"/>
	</db-entity>
	<db-entity name="USER">
		<db-attribute name="ENABLED" type="BOOLEAN"/>
		<db-attribute name="FK_AUTH_ADAPTER" type="INTEGER"/>
		<db-attribute name="FULL_NAME" type="VARCHAR" length="200"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="TIME_CREATE" type="TIMESTAMP"/>
		<db-attribute name="TIME_MODIFY" type="TIMESTAMP"/>
		<db-attribute name="USERNAME" type="VARCHAR" length="200"/>
		<db-attribute name="USR_CREATE" type="VARCHAR" length="30"/>
		<db-attribute name="USR_MODIFY" type="INTEGER" length="30"/>
	</db-entity>
	<db-entity name="USER_ROLE">
		<db-attribute name="BRANCH" type="VARCHAR" length="30"/>
		<db-attribute name="FK_ROLE" type="INTEGER"/>
		<db-attribute name="FK_USER" type="INTEGER"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="SECTION" type="VARCHAR" length="30"/>
		<db-attribute name="TIME_CREATE" type="TIMESTAMP"/>
		<db-attribute name="TIME_MODIFY" type="TIMESTAMP"/>
		<db-attribute name="USR_CREATE" type="VARCHAR" length="30"/>
		<db-attribute name="USR_MODIFY" type="VARCHAR" length="30"/>
	</db-entity>
	<db-relationship name="auth_AdapterReverse" source="AUTH_ADAPTER" target="USER" toMany="true">
		<db-attribute-pair source="ID" target="FK_AUTH_ADAPTER"/>
	</db-relationship>
	<db-relationship name="XPERMISSION" source="ROLE_PERMISSION" target="PERMISSION" toMany="false">
		<db-attribute-pair source="ID" target="ID"/>
	</db-relationship>
	<db-relationship name="XROLE" source="ROLE_PERMISSION" target="ROLE" toMany="false">
		<db-attribute-pair source="FK_ROLE" target="ID"/>
	</db-relationship>
	<db-relationship name="Auth_Adapter" source="USER" target="AUTH_ADAPTER" toMany="false">
		<db-attribute-pair source="FK_AUTH_ADAPTER" target="ID"/>
	</db-relationship>
</data-map>
